{
  "compilerOptions": {
    "allowJs": false,
    "module": "commonjs",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2019",
    "importHelpers": false,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": false,
    "declaration": false,
    "rootDir": ".",
    "outDir": "dist"
  },
  "include": [
    "./**/*.ts",
  ],
  "exclude": [
    "node_modules/**/*",
    "dist"
  ]
}